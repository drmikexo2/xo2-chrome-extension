{
    "name": "xo2.com",
    "version": "0.2.4",
    "description": "xo2.com crypto twitter intelligence integration",
    "manifest_version": 3,
    "host_permissions": [
        "*://twitter.com/*"
    ],
    "icons": {
        "16": "favicon-16x16.png",
        "32": "favicon-32x32.png",
        "48": "favicon.ico"
      },
    "content_scripts": [{
        "matches": [
            "*://twitter.com/*"
        ],
        "js": [
            "script.js"
        ],
        "run_at": "document_idle",
        "all_frames": true
    }]
}